{
  "version": 2,
  "builds": [
    {
      "src": "src/api/techs.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/src/api/techs.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
