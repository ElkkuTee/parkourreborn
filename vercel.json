{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["api/_utils/**"]
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/auth/discord",
      "dest": "api/auth/discord/index.js"
    },
    {
      "src": "/api/auth/discord/callback",
      "dest": "api/auth/discord/callback.js"
    },
    {
      "src": "/api/send-request",
      "dest": "api/send-request.js"
    },
    {
      "src": "/api/techs",
      "dest": "api/techs.js"
    },
    {
      "src": "/api/techs/(.*)",
      "dest": "api/techs.js"
    }
  ]
}