{
  "version": 2,
  "builds": [
    {
      "src": "api/techs.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/techs",
      "dest": "api/techs.js"
    },
    {
      "src": "/api/techs/(.*)",
      "dest": "api/techs.js"
    }
  ]
}